<%= form_with url: "/users", method: :post, local: true do |f| %>
  <%= f.label :name, 'Name' %>
  <%= f.text_field :name %>
  <%= f.label :email, 'Email' %>
  <%= f.text_field :email %>
  <%= f.label :user_name, 'Username' %>
  <%= f.text_field :user_name %>
  <%= f.label :password, 'Password' %>
  <%= f.password_field :password %>
  <%= f.submit "Register New User" %>
<% end %>
